{
  "name": "beriox-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3000",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "echo ESLint temporairement désactivé",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:push": "prisma db push",
    "worker": "tsx --env-file=.env src/worker.ts",
    "compose": "docker compose up -d",
    "qa": "node scripts/test-qa-optimized.js",
    "qa:legacy": "node qa-test.js",
    "security": "node scripts/test-security-optimized.js",
    "security:legacy": "node security-audit.js",
    "test:complete": "node scripts/test-complete-optimized.js",
    "test:all": "npm run test:complete",
    "test:forms": "node scripts/test-forms.js",
    "test:rate-limit": "node scripts/test-rate-limiting.js",
    "test:cache": "node scripts/test-cache.js",
    "test:super-admin": "node scripts/test-super-admin.js",
    "test:time-tracking": "node scripts/test-time-tracking.js",
    "test:monitoring": "node scripts/test-monitoring.js",
    "test:notifications": "node scripts/test-notifications.js",
    "test:ab-testing": "node scripts/test-ab-testing.js",
    "test:onboarding": "node scripts/test-onboarding.js",
    "test:form-optimization": "node scripts/test-form-optimization.js",
    "test:access-control": "node scripts/test-access-control.js",
    "test:authentication": "node scripts/test-authentication.js",
    "test:automated": "npm run test:complete",
    "test:responsive": "node scripts/test-responsive.js",
    "setup-super-admin": "node scripts/setup-super-admin.js",
    "reset-credits": "node scripts/reset-credits.js",
    "dev:ignore-lint": "node scripts/dev-ignore-lint.js",
    "test:orchestration": "node scripts/test-orchestration.js",
    "test:orchestration-direct": "node scripts/test-orchestration-direct.js",
    "test:simple-orchestration": "node scripts/test-simple-orchestration.js",
    "test:bots": "node scripts/test-bots.js",
    "test:html-fixer": "node scripts/test-bots.js html",
    "test:linter-fixer": "node scripts/test-bots.js linter",
    "fix:html": "node scripts/test-bots.js html --apply",
    "fix:linter": "node scripts/test-bots.js linter --apply",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:staging": "vercel --env staging",
    "deploy:prod": "vercel --prod",
    "health:check": "curl -f $VERCEL_URL/api/health || exit 1",
    "health:monitor": "curl -f $VERCEL_URL/api/monitoring/health || exit 1",
    "test:e2e:deployment": "playwright test tests/e2e/deployment.spec.ts",
    "monitor:start": "node scripts/monitor.js",
    "rollback": "vercel rollback",
    "fix:lint": "./scripts/fix-lint.sh",
    "fix:lint:manual": "npm run lint -- --fix"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@notionhq/client": "^4.0.1",
    "@prisma/client": "^6.13.0",
    "@sentry/nextjs": "^10.4.0",
    "@slack/web-api": "^7.9.3",
    "@stripe/stripe-js": "^7.8.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.56.9",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express-rate-limit": "^8.0.1",
    "ioredis": "^5.7.0",
    "jsonrepair": "^3.13.0",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "openai": "^5.12.2",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "puppeteer": "^24.16.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.0.1",
    "stripe": "^18.4.0",
    "thread-stream": "^3.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.6",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-istanbul": "^7.0.0",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.4.6",
    "glob": "^11.0.3",
    "inflight": "^1.0.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "prisma": "^6.13.0",
    "tailwindcss": "^4.1.11",
    "test-exclude": "^7.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "glob": "^11.0.3",
    "inflight": "^1.0.6"
  }
}
